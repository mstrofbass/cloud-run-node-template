{
  "name": "cloud-run-node-service",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec 'dotenv -e ./.env -- npm start' ",
    "start": "npx node ./bin/www",
    "lint-check": "prettier --ignore-unknown --check '*/**' && eslint . --ext .js --ext .ts",
    "lint-staged:jest": "jest --bail --findRelatedTests --passWithNoTests",
    "prepare": "husky install",
    "test:e2e": "dotenv -e .env.ci -- jest ./spec/e2e",
    "test:integration": "jest ./spec/integration",
    "test:non-e2e": "jest ./spec/unit ./spec/integration",
    "test:unit": "jest ./spec/unit",
    "test:watch": "jest --testPathIgnorePatterns='spec/e2e/*' --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DecentHealth/quote-service-js.git"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "express": "^4.18.2",
    "agentkeepalive": "^4.2.1",
    "cookie-parser": "~1.4.4",
    "csv": "^6.2.5",
    "debug": "~2.6.9",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "optionalDependencies": {
    "husky": "^8.0.2"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "{app.js,src/**/*.js}": [
      "eslint --fix",
      "npm run lint-staged:jest"
    ],
    "spec/**/*.js": "eslint --fix"
  }
}
